<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/box_modale">

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/electroniciconcontainer"
        app:layout_constraintHorizontal_chainStyle="packed"
        android:layout_marginTop="66dp"
        android:layout_marginEnd="56dp"
        android:background="@android:color/transparent"
        android:src="@drawable/icon_circlebtndefault"
        android:id="@+id/gasiconcontainer"/>

    <ImageView
        android:layout_width="22dp"
        android:layout_height="22dp"
        app:layout_constraintStart_toStartOf="@id/gasiconcontainer"
        app:layout_constraintEnd_toEndOf="@id/gasiconcontainer"
        app:layout_constraintTop_toTopOf="@id/gasiconcontainer"
        app:layout_constraintBottom_toBottomOf="@id/gasiconcontainer"
        android:background="@android:color/transparent"
        android:src="@drawable/billicon_gas_default"
        android:scaleType="centerInside"
        android:id="@+id/gasicon"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@id/gasiconcontainer"
        app:layout_constraintStart_toStartOf="@id/gasiconcontainer"
        app:layout_constraintTop_toBottomOf="@id/gasiconcontainer"
        android:layout_marginTop="10dp"
        android:textSize="16dp"
        android:text="가스"
        android:textColor="@color/black_text"
        android:fontFamily="@font/suit_medium"
        android:id="@+id/gasname"/>

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:layout_constraintTop_toTopOf="@id/gasiconcontainer"
        app:layout_constraintStart_toEndOf="@id/gasiconcontainer"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@android:color/transparent"
        android:src="@drawable/icon_circlebtndefault"
        android:id="@+id/electroniciconcontainer"/>

    <ImageView
        android:layout_width="22dp"
        android:layout_height="22dp"
        app:layout_constraintStart_toStartOf="@id/electroniciconcontainer"
        app:layout_constraintEnd_toEndOf="@id/electroniciconcontainer"
        app:layout_constraintTop_toTopOf="@id/electroniciconcontainer"
        app:layout_constraintBottom_toBottomOf="@id/electroniciconcontainer"
        android:background="@android:color/transparent"
        android:src="@drawable/billicon_electronic_default"
        android:scaleType="centerInside"
        android:id="@+id/electronicicon"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@id/electroniciconcontainer"
        app:layout_constraintStart_toStartOf="@id/electroniciconcontainer"
        app:layout_constraintTop_toBottomOf="@id/electroniciconcontainer"
        android:layout_marginTop="10dp"
        android:textSize="16dp"
        android:text="전기"
        android:textColor="@color/black_text"
        android:fontFamily="@font/suit_medium"
        android:id="@+id/electronicname"/>

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:layout_constraintTop_toBottomOf="@id/gasname"
        app:layout_constraintStart_toStartOf="@id/gasiconcontainer"
        app:layout_constraintEnd_toEndOf="@id/gasiconcontainer"
        android:layout_marginTop="30dp"
        android:background="@android:color/transparent"
        android:src="@drawable/icon_circlebtndefault"
        android:id="@+id/watericoncontainer"/>

    <ImageView
        android:layout_width="25dp"
        android:layout_height="25dp"
        app:layout_constraintStart_toStartOf="@id/watericoncontainer"
        app:layout_constraintEnd_toEndOf="@id/watericoncontainer"
        app:layout_constraintTop_toTopOf="@id/watericoncontainer"
        app:layout_constraintBottom_toBottomOf="@id/watericoncontainer"
        android:background="@android:color/transparent"
        android:src="@drawable/billicon_water_default"
        android:scaleType="centerInside"
        android:id="@+id/watericon"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@id/watericoncontainer"
        app:layout_constraintStart_toStartOf="@id/watericoncontainer"
        app:layout_constraintTop_toBottomOf="@id/watericoncontainer"
        android:layout_marginTop="10dp"
        android:textSize="16dp"
        android:text="수도"
        android:textColor="@color/black_text"
        android:fontFamily="@font/suit_medium"
        android:id="@+id/watername"/>

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:layout_constraintTop_toBottomOf="@id/electronicname"
        app:layout_constraintStart_toStartOf="@id/electroniciconcontainer"
        app:layout_constraintEnd_toEndOf="@id/electroniciconcontainer"
        android:layout_marginTop="30dp"
        android:background="@android:color/transparent"
        android:src="@drawable/icon_circlebtndefault"
        android:id="@+id/etciconcontainer"/>

    <ImageView
        android:layout_width="25dp"
        android:layout_height="25dp"
        app:layout_constraintStart_toStartOf="@id/etciconcontainer"
        app:layout_constraintEnd_toEndOf="@id/etciconcontainer"
        app:layout_constraintTop_toTopOf="@id/etciconcontainer"
        app:layout_constraintBottom_toBottomOf="@id/etciconcontainer"
        android:background="@android:color/transparent"
        android:src="@drawable/billicon_etc_default"
        android:scaleType="centerInside"
        android:id="@+id/etcicon"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@id/etciconcontainer"
        app:layout_constraintStart_toStartOf="@id/etciconcontainer"
        app:layout_constraintTop_toBottomOf="@id/etciconcontainer"
        android:layout_marginTop="10dp"
        android:textSize="16dp"
        android:text="기타"
        android:textColor="@color/black_text"
        android:fontFamily="@font/suit_medium"
        android:id="@+id/etcname"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="65dp"
        app:layout_constraintTop_toBottomOf="@id/watername"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="28dp"
        android:layout_marginTop="43dp"
        android:id="@+id/confirmbtn"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:background="@drawable/box_modalebutton"
        android:text="사진 스캔"
        android:textColor="@color/white"
        android:fontFamily="@font/suit_bold"
        android:textSize="18dp"
        android:stateListAnimator="@null"/>

</androidx.constraintlayout.widget.ConstraintLayout>