<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/graylight_basic">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@drawable/textbox_whitelist"
        android:layout_marginBottom="12dp"
        android:layout_marginRight="20dp"
        android:layout_marginLeft="20dp">

        <ImageView
            android:id="@+id/mainlistprofile"
            android:layout_width="60dp"
            android:layout_height="60dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="18dp"
            android:layout_marginLeft="16dp"
            android:src="@drawable/basicprofile"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toRightOf="@id/mainlistprofile"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="14dp"
            android:layout_marginTop="14dp"
            android:text="TEST"
            android:textColor="@color/black"
            android:textSize="16dp"
            android:id="@+id/listmaintext"
            android:fontFamily="@font/suit_bold_apply"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toRightOf="@id/mainlistprofile"
            app:layout_constraintTop_toBottomOf="@id/listmaintext"
            android:layout_marginLeft="14dp"
            android:layout_marginTop="3dp"
            android:text="TEST"
            android:textSize="14dp"
            android:id="@+id/listsubtext"
            android:textColor="@color/graydark_text"
            android:fontFamily="@font/suit_semibold_apply"
            />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/listsubtext"
            android:layout_marginStart="90dp"
            android:layout_marginEnd="57dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="20dp"
            android:id="@+id/mainlistgraph"
            android:background="@drawable/graph_graybackground"
            app:layout_constraintWidth_default="spread">

            <androidx.constraintlayout.widget.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.6"
                android:id="@+id/mainlistguide"
                />

            <ImageView
                android:layout_width="0dp"
                android:layout_height="10dp"
                app:layout_constraintEnd_toEndOf="@id/mainlistguide"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:src="@drawable/graph_bluebar14"
                android:id="@+id/mainlistbar"
                />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/listsubtext"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/mainlistgraph"
            android:layout_marginRight="16dp"
            android:layout_marginLeft="5dp"
            android:layout_marginBottom="10dp"
            android:id="@+id/percentText"
            android:textSize="16dp"
            android:gravity="right"
            android:textColor="@color/graydark_text"
            android:text="60%"
            android:fontFamily="@font/montserrat_bold"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>